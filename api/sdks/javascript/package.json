{
  "name": "@go-objstore/client",
  "version": "0.1.0",
  "description": "JavaScript SDK for go-objstore - Unified object storage client supporting REST, gRPC, and QUIC/HTTP3 protocols",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run clean && npm run build:proto && npm run build:src",
    "build:proto": "node scripts/generate-proto.js",
    "build:src": "rollup -c",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest tests/unit --coverage",
    "test:unit": "NODE_OPTIONS=--experimental-vm-modules jest tests/unit --coverage",
    "test:integration": "NODE_OPTIONS=--experimental-vm-modules jest tests/integration --runInBand --testTimeout=90000",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "clean": "rm -rf dist coverage .jest-cache",
    "lint": "eslint src tests",
    "lint:fix": "eslint src tests --fix",
    "format": "prettier --write \"src/**/*.js\" \"tests/**/*.js\"",
    "coverage": "npm run test:unit && npx http-server coverage/lcov-report -o",
    "prepublishOnly": "npm run build && npm test"
  },
  "keywords": [
    "object-storage",
    "s3",
    "gcs",
    "azure",
    "storage",
    "grpc",
    "rest",
    "quic",
    "http3"
  ],
  "author": "Go ObjectStore",
  "license": "AGPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/jeremyhahn/go-objstore.git",
    "directory": "api/sdks/javascript"
  },
  "bugs": {
    "url": "https://github.com/jeremyhahn/go-objstore/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.9.14",
    "@grpc/proto-loader": "^0.7.10",
    "axios": "^1.6.2",
    "form-data": "^4.0.0",
    "google-protobuf": "^3.21.2"
  },
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "@babel/preset-env": "^7.23.5",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "axios-mock-adapter": "^2.1.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest": "^27.6.0",
    "grpc_tools_node_protoc_ts": "^5.3.3",
    "grpc-tools": "^1.12.4",
    "http-server": "^14.1.1",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "rollup": "^4.6.1"
  },
  "browser": {
    "fs": false,
    "net": false,
    "tls": false,
    "http2": false
  }
}
